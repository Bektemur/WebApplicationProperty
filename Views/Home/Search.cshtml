@{ ViewData["Title"] = "Home Page";}@using WebApplicationProperty.ViewModel@model IndexViewModel<div class="row mb-1">    <div class="col-md-12">        <form data-ajax="true" data-ajax-mode="replace" method="post" data-ajax-update="#theListContent" data-ajax-url="@Url.Action("SearchResult","Home")" class="w-100">            <nav class="navbar navbar-expand-lg navbar-light bg-light">                <button class="navbar-toggler" type="submit" data-toggle="collapse" data-target="#navbarTogglerSearcher" aria-controls="navbarTogglerSearcher" aria-expanded="false" aria-label="Toggle navigation">                    <span class="navbar-toggler-icon"></span>                </button>                <div class="collapse navbar-collapse" id="navbarTogglerSearcher">                    <ul class="navbar-nav">                        <li class="nav-item">                            <span>Contract types</span><br />                            <select class="custom-select" onchange="propertyChange()" asp-for="@Model.PropertyContractType" asp-items="Html.GetEnumSelectList<PropertyContractType>()"></select>                        </li>                        <li class="nav-item ml-2" id="divPriceSale">                            <span>Price for sale</span><br />                            <select class="custom-select" asp-for="@Model.PriceForSale" asp-items="Html.GetEnumSelectList<PriceForSale>()"></select>                        </li>                        <li class="nav-item ml-2" id="divPriceRent">                            <span>Price for rent</span><br />                            <select class="custom-select" asp-for="@Model.PriceForRent" asp-items="Html.GetEnumSelectList<PriceForRent>()"></select>                        </li>                        <li class="nav-item ml-1">                            <span>Bedrooms</span><br />                            <select class="custom-select" id="propertyBedrooms" name="propertyBedrooms" asp-for="@Model.Bedrooms" asp-items="Html.GetEnumSelectList<BedRooms>()">                            </select>                        </li>                        @* <li class="nav-item">                                <span>Province</span><br />                                <select name="province" style="margin-left:0px;" id="province">                                    <option value="sdcsdcsd">erververvee</option>                                    <option value="sdcsdcsd">verververvevev</option>                                    <option value="sdcsdcsd">erverveverveev</option>                                    <option value="sdcsdcsd">erververve</option>                                    <option value="sdcsdcsd">ervvrervev</option>                                </select>                            </li>                            <li class="nav-item">                                <span>BTS/MRT station</span><br />                                <select asp-for="@Model.Station" style="margin-left:0px;"  asp-items="ViewBag.StationId"></select>                            </li>*@                        <li class="nav-item ml-1">                            <span>Property types</span><br />                            <select class="custom-select" id="propertyType" name="propertyType" asp-for="@Model.PropertyType" asp-items="Html.GetEnumSelectList<PropertyType>()">                            </select>                        </li>                    </ul>                    <div class="ml-3 w-50">                        <div><small class="float-left pb-1">* For a detailed search or to list your property just <a asp-area="Identity" asp-page="/Account/Register">register</a>.</small></div>                        <div class="d-inline-flex w-100">                            <input class="form-control mr-sm-2" type="text" id="search" name="search" placeholder="ex. Address ..." aria-label="Search" style="min-width:300px;">                            <input class="btn btn-success ml-2" type="submit" value="Search" id="submitFormBtn">                        </div>                    </div>                </div>            </nav>        </form>    </div></div><div class="row">    @if (User.IsInRole(Roles.Admin))    {        <div class="col-sm-2" id="improvement">            <p>Facilities:</p>        </div>    }    <div class="@(User.IsInRole(Roles.Admin)?"col-sm-10":"col-sm-12")">        <nav aria-label="Page navigation">            <ul class="pagination justify-content-center">                <li class="page-item disabled">                    <a class="page-link" href="#" tabindex="-1">Previous</a>                </li>                <li class="page-item @(Model.Page==1?"active":"")"><a class="page-link" href="?page=1&take=25">1</a></li>                <li class="page-item @(Model.Page==2?"active":"")"><a class="page-link" href="?page=2&take=25">2</a></li>                <li class="page-item @(Model.Page==3?"active":"")"><a class="page-link" href="?page=3&take=25">3</a></li>                <li class="page-item">                    <a class="page-link" href="#">Next</a>                </li>            </ul>        </nav>        <div id="theListContent" class="col-12 d-inline">            <partial name='_PropertySearchPartial' model='Model' />        </div>        <nav aria-label="Page navigation">            <ul class="pagination justify-content-center">                <li class="page-item disabled">                    <a class="page-link" href="#" tabindex="-1">Previous</a>                </li>                <li class="page-item @(Model.Page==1?"active":"")"><a class="page-link" href="?page=1&take=25">1</a></li>                <li class="page-item @(Model.Page==2?"active":"")"><a class="page-link" href="?page=2&take=25">2</a></li>                <li class="page-item @(Model.Page==3?"active":"")"><a class="page-link" href="?page=3&take=25">3</a></li>                <li class="page-item">                    <a class="page-link" href="#">Next</a>                </li>            </ul>        </nav>    </div></div>@section Scripts{    <partial name="_ValidationScriptsPartial" />    <partial name="_Ajax" />    <script type="text/javascript">        $(document).ready(function () {            $("#divPriceSale").hide();            $('#PropertyContractType').change(function () {                $("#submitFormBtn").click();            });            $('#propertyType').change(function () {                $("#submitFormBtn").click();            });            $('#propertyBedrooms').change(function () {                $("#submitFormBtn").click();            });            $('#PriceForSale').change(function () {                $("#submitFormBtn").click();            });            $('#PriceForRent').change(function () {                $("#submitFormBtn").click();            });        });        function propertyChange() {            var propertyContractType = $("#PropertyContractType").val();            if (propertyContractType == 0) {                $("#divPriceSale").hide();                $("#divPriceRent").show();            }            if (propertyContractType == 1) {                $("#divPriceRent").hide();                $("#divPriceSale").show();            }        }    </script>}