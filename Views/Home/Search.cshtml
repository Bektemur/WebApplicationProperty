@{ ViewData["Title"] = "Home Page";}@using WebApplicationProperty.ViewModel@model IndexViewModel<div class="row">    <div class="col-md-12">        <form data-ajax="true" data-ajax-mode="replace" method="post" data-ajax-update="#theListContent" data-ajax-url="@Url.Action("SearchResult","Home")" class="w-100">            <nav class="navbar navbar-expand-lg navbar-light bg-light">                <button class="navbar-toggler" type="submit" data-toggle="collapse" data-target="#navbarTogglerDemo03" aria-controls="navbarTogglerDemo03" aria-expanded="false" aria-label="Toggle navigation">                    <span class="navbar-toggler-icon"></span>                </button>                <div class="collapse navbar-collapse" id="navbarTogglerDemo03">                    <ul class="navbar-nav">                        <li class="nav-item">                            <span>Contract types</span><br />                            <select id="propertyContractType" style="margin-right:10px; position:relative;" onchange="propertyChange()" name="propertyContractType" asp-for="@Model.PropertyContractType" asp-items="Html.GetEnumSelectList<PropertyContractType>()"></select>                        </li>                        <li class="nav-item" id="pricesale" style="margin-left:10px;">                            <span>Price for sale</span><br />                            <select asp-for="@Model.PriceForSale" name="PriceForSale" id="PriceForSale" asp-items="Html.GetEnumSelectList<PriceForSale>()"></select>                        </li>                        <li class="nav-item" id="pricerent" style="margin-left:10px;">                            <span>Price for rent</span><br />                            <select id="PriceForRent" asp-for="@Model.PriceForRent" name="PriceForRent" asp-items="Html.GetEnumSelectList<PriceForRent>()"></select>                        </li>                        <li class="nav-item" style="margin-left:10px;">                            <span>Bedrooms</span><br />                            <select id="propertyBedrooms" name="propertyBedrooms" asp-for="@Model.Bedrooms" asp-items="Html.GetEnumSelectList<BedRooms>()">                            </select>                        </li>                        @* <li class="nav-item">                                <span>Province</span><br />                                <select name="province" style="margin-left:0px;" id="province">                                    <option value="sdcsdcsd">erververvee</option>                                    <option value="sdcsdcsd">verververvevev</option>                                    <option value="sdcsdcsd">erverveverveev</option>                                    <option value="sdcsdcsd">erververve</option>                                    <option value="sdcsdcsd">ervvrervev</option>                                </select>                            </li>                            <li class="nav-item">                                <span>BTS/MRT station</span><br />                                <select asp-for="@Model.Station" style="margin-left:0px;"  asp-items="ViewBag.StationId"></select>                            </li>*@                        <li class="nav-item" style="margin-left:10px;">                            <span>Property types</span><br />                            <select id="propertyType" style="margin-left:0px;" name="propertyType" asp-for="@Model.PropertyType" asp-items="Html.GetEnumSelectList<PropertyType>()">                            </select>                        </li>                    </ul>                    <div class="form-inline my-2 my-lg-0" style="margin-left:10px;">                        <input class="form-control mr-sm-2" type="text" id="search" name="search" placeholder="Search" aria-label="Search">                        <input class="btn btn-outline-success my-2 my-sm-0" type="submit" value="Search" id="submitFormBtn" />                    </div>                </div>            </nav>        </form>    </div></div><div class="row">    @if (User.IsInRole(Roles.Admin))    {<div class="col-sm-2" id="improvement">    <p></p></div>}    <div class="@(User.IsInRole(Roles.Admin)?"col-sm-9":"col-sm-12")">        <nav aria-label="Page navigation example">            <ul class="pagination justify-content-center">                <li class="page-item disabled">                    <a class="page-link" href="#" tabindex="-1">Previous</a>                </li>                <li class="page-item @(Model.Page==1?"active":"")"><a class="page-link" href="?page=1&take=25">1</a></li>                <li class="page-item @(Model.Page==2?"active":"")"><a class="page-link" href="?page=2&take=25">2</a></li>                <li class="page-item @(Model.Page==3?"active":"")"><a class="page-link" href="?page=3&take=25">3</a></li>                <li class="page-item">                    <a class="page-link" href="#">Next</a>                </li>            </ul>        </nav>        <div id="theListContent" class="col-12 d-inline">            <partial name='_PropertySearchPartial' model='Model' />        </div>        <nav aria-label="Page navigation example">            <ul class="pagination justify-content-center">                <li class="page-item disabled">                    <a class="page-link" href="#" tabindex="-1">Previous</a>                </li>                <li class="page-item @(Model.Page==1?"active":"")"><a class="page-link" href="?page=1&take=25">1</a></li>                <li class="page-item @(Model.Page==2?"active":"")"><a class="page-link" href="?page=2&take=25">2</a></li>                <li class="page-item @(Model.Page==3?"active":"")"><a class="page-link" href="?page=3&take=25">3</a></li>                <li class="page-item">                    <a class="page-link" href="#">Next</a>                </li>            </ul>        </nav>    </div></div>@section Scripts{    <partial name="_ValidationScriptsPartial" />    <script src="~/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.js"></script>    <script type="text/javascript">        document.getElementById("pricesale").style.visibility = "visible";        $('#propertyContractType').change(function () {            $("#submitFormBtn").click();        });        $('#propertyType').change(function () {            $("#submitFormBtn").click();        });        $('#propertyBedrooms').change(function () {            $("#submitFormBtn").click();        });        $('#PriceForSale').change(function () {            $("#submitFormBtn").click();        });        $('#PriceForRent').change(function () {            $("#submitFormBtn").click();        });        var priceRent = document.getElementById("pricerent").value;        function propertyChange() {            var propertyContractType = document.getElementById("propertyContractType").value;            if (propertyContractType == 1) {                document.getElementById("pricerent").style.visibility = "visible";                document.getElementById("pricesale").style.visibility = "hidden";            }            if (propertyContractType == 2) {                document.getElementById("pricerent").style.visibility = "hidden";                document.getElementById("pricesale").style.visibility = "visible";            }        }    </script>}